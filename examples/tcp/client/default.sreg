   EndTime: 70
   EndValue: 5.0

  SeriesTrajectory:
   StartTime: 5
   StartValue: 28.0
   EndTime: 70
   EndValue: 5.0

  Test:
    Value1: 11.2
    Value2: 22.1
    Value3: 4
    Value4: 1

  Remote:
    PushExample:
      Module: Test
      Parameter: Value1
      valueOn: '99.99'
      widgetType: PushButton
      shortcut: P
    SwitchExample:
      Module: Test
      Parameter: Value2
      valueOff: '11.1'
      valueOn: '22.2'
      widgetType: Switch
      shortcut: O
